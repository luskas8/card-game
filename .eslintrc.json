{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "eslint-plugin-prettier",
        "react"
    ],
    "ignorePatterns":[
        "node_modules/",
        "build/",
        "dist/",
        "coverage/",
        "public/",
        "scripts/",
        "package-lock.json"
    ],
    "rules": {
        "indent": "error"
    }
}
